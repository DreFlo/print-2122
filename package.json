{
  "name": "Sigtools",
  "version": "2.1.2",
  "description": "Tools for sigarra",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist:win": "electron-builder -w 7z",
    "sass": "sass --watch ./_views/css/style.scss ./_views/css/style.css",
    "doc": "jsdoc _linkers/ -r -c jsdoc.json -d docs"
  },
  "build": {
    "appId": "com.Sigtools.app",
    "asar" : false,
    "npmRebuild": false,
    "files" : [
      "_engine/",
      "_linkers/",
      "_views/",
      "js/",
      "index.html",
      "main.js",
      "webdev/",
      "lib/",
      "rederer.js",
      "preload.js",
      "data/",
      "logged",
      "images/",
      "public/",
      "color.less"
    ]
  },
  "keywords": [
    "Electron"
  ],
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^8.0.3",
    "electron-builder": "^22.4.1",
    "electron-packager": "^14.2.1",
    "jsdoc": "^3.6.5"
  },
  "bugs": {
    "url": "https://git.fe.up.pt/printegrador/2022/pe38-sigarratools"
  },
  "homepage": "https://git.fe.up.pt/printegrador/2022/pe38-sigarratools",
  "dependencies": {
    "clipboard": "^2.0.6",
    "lodash.deburr": "^4.1.0",
    "node-sass": "^6.0.1",
    "normalizer": "^1.2.7",
    "python": "^0.0.4",
    "python-shell": "^1.0.8",
    "require": "^2.4.20",
    "table2csv": "^1.1.4",
    "toastify-js": "^1.10.0"
  }
}
